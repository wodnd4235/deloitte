<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

	<context id="context1" targetRuntime="MyBatis3Simple" defaultModelType="flat">

		<commentGenerator>
		        <property name="suppressAllComments" value="false"/>
		        <property name="suppressDate" value="true"/>
	    </commentGenerator>
		
		<!-- <jdbcConnection
			connectionURL="jdbc:postgresql://xx.xx.xx:5432/oc_dev"
			driverClass="org.postgresql.Driver" password="passsss"
			userId="user" /> -->
		
		<jdbcConnection
			connectionURL="jdbc:h2:~/test"
			driverClass="org.h2.Driver" password="1234"
			userId="choi" />
			
		<javaModelGenerator targetPackage="com.deloitte.solution.entity" targetProject="Deloitte/src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		
		<sqlMapGenerator targetPackage="mapper" targetProject="Deloitte/src/main/resources/mapper"/>
		
		<javaClientGenerator targetPackage="mapper" targetProject="Deloitte/src/main/resources/mapper" type="XMLMAPPER">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>
		
		<table tableName="TEST" domainObjectName="Test">
			<!-- <ignoreColumn column="reg_dt" /> -->
		</table>
	
	</context>

</generatorConfiguration>
